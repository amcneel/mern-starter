  "scripts": {
    "bs": "npm run clean && npm run build && npm run prod",
    "build": "cross-env NODE_ENV=production webpack -p --progress --config webpack.config.prod.js",
    "clean": "rimraf dist",
    "lint": "node node_modules/.bin/tslint -c tslint.json --project tsconfig.json 'client/**/*.ts?(x)' 'server/**/*.ts?(x)'",
    "start": "npm run clean && npm run build && npm run prod",
    "dev": "cross-env BABEL_DISABLE_CACHE=1 NODE_ENV=development nodemon",
    "prod": "cross-env NODE_ENV=production ts-node index.ts",
    "watch-ts": "tsc -w"
  },
